{
  "name": "us-census-app",
  "private": true,
  "version": "0.0.1",
  "description": "Display US census",
  "devDependencies": {
    "bower": "1.3.x",
    "http-server": "0.6.x",
    "protractor": "1.1.x"
  },
  "scripts": {
    "postinstall": "bower install",
    "start": "http-server app -a 0.0.0.0 -p 8000 -c-1 --cors ",
